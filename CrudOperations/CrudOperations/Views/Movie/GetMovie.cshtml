    @model List<DAL.DataModel.Movie>
@{
    ViewData["Title"] = "GetMovie";
}

<h1>GetMovie</h1>

<div class="table">
    <table>
        <tr>
            <th>Title</th>
            <th>Geners</th>
            <th>Type</th>
            <th>MinimumAge</th>
            <th>Action</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>    
                <td>@item.Title</td>
                <td>@item.Generes</td>
                <td>@item.Type</td>
                <td>@item.MinimumAge</td>
                <td>
                    @Html.ActionLink("Edit", "EditMovie", new { id = item.id })
                </td>
                
            </tr>
        }
    </table>
</div>